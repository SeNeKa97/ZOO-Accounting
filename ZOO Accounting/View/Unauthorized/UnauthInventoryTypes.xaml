<Window x:Class="ZOO_Accounting.View.Unauthorized.UnauthInventoryTypes"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Типы инвентаря" Height="312.766" Width="321.7" ResizeMode="CanMinimize">
    <Grid>
        <DataGrid Name="grid" ItemsSource="{Binding inventoryTypes}" AutoGenerateColumns="False" IsReadOnly="True" HorizontalAlignment="Left" Height="250" Margin="10,10,0,0" VerticalAlignment="Top" Width="274">
            <DataGrid.Columns>
                <DataGridTextColumn Binding="{Binding Id}" Header="ID"></DataGridTextColumn>
                <DataGridTextColumn Binding="{Binding Name}" Header="Название"></DataGridTextColumn>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>

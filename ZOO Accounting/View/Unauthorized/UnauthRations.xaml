<Window x:Class="ZOO_Accounting.View.Unauthorized.UnauthRations"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Рационы" Height="317.021" Width="321.317" ResizeMode="CanMinimize">
    <Grid>
        <DataGrid Name="grid" ItemsSource="{Binding rations}" AutoGenerateColumns="False" IsReadOnly="True" HorizontalAlignment="Left" Height="250" Margin="10,10,0,0" VerticalAlignment="Top" Width="274">
            <DataGrid.Columns>
                <DataGridTextColumn Binding="{Binding Name}" Header="Название"></DataGridTextColumn>
                <DataGridTextColumn Binding="{Binding Ingredients}" Header="Ингредиенты"></DataGridTextColumn>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
